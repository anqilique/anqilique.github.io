> object music javascript
    var command = args.join(" ");
    const audioPlayer = document.getElementById('backgroundMusic');

    if (localStorage.audio_enabled === 'true' && (command === 'stop')) {
        audioPlayer.pause();
        audioPlayer.currentTime = 0;

        localStorage.audio_enabled = 'false';
        return "Background music stopped.";

    } else if (localStorage.audio_enabled === 'true' && command === 'pause') {
        localStorage.audio_enabled = 'false';
        audioPlayer.pause();

        return "Background music paused.";

    } else if (localStorage.audio_enabled === 'false' && (command === 'play')) {
        audioPlayer.play();
        localStorage.audio_enabled = 'true';
        return "Playing <span style='color: var(--blue)'>eggs on toast</span> by <a href='https://youtu.be/WwpMT6b9Rys?list=PLjmaGYAtBvtexABXfqNfI0RaGc8kD0z3c' target='_blank'>a bowl of rice ðŸŽµ</a> <span style='color: var(--silver)'> <-- Click to support the artist! </span>";
    }

    else {
        return "You can't do that right now, or invalid command."
    }

